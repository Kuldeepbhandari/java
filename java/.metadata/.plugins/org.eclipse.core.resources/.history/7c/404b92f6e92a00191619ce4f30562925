package AssingmentDay1;

public class TimeTable {
public static void show()
{

	int t=0;
	for(int a=0;a<=9;a++)
	{if(a==0)
		System.out.print("\t " +"*");
	else
		System.out.print("\t"+a);
	
	}
	System.out.println("");
	System.out.println("\t" +" -- - - - - - -  - -  - -  - -");
	for(int i=1;i<=9;i++)
	{	
		System.out.print("\t"+i);
	    System.out.print(" |");
		for(int j=1;j<=9;j++)
		{
			t=i*j;
			System.out.print("\t"+t);
		}
		
		System.out.println(" ");
	}
}

}
