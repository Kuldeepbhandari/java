package AssingmentDay1;

public class Pattern {
  public static void show()
  {int n=6;
	  for(int i=1;i<=n;i++)
	  {
		  for(int j=1;j<=n;j++)
		  {		
			  if(i==1||i==n|| i==2||i==n-1||j==1||j==2||j==4||j==n||j==n-1)
				  	System.out.print("*");
		   else
			   		System.out.println("  ");
		   }
		  System.out.println("");
	  }
	 
  }
}
