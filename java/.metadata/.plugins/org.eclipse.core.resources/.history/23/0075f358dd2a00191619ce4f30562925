package AssingmentDay1;

public class Pattern {
  public static void show()
  {
	  for(int i=1;i<=6;i++)
	  {
		  for(int j=1;j<=6;j++)
		  {
			 if(j==1)
			 {
				 System.out.print("*");
			 }
			 else if(j==2||j==3||j==4||j==5){
				 System.out.println("**  **");
				 break;
			 }
		  }
	  }
	  System.out.println(" ");
  }
}
