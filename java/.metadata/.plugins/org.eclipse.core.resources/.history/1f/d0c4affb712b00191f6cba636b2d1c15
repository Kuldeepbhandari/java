package AssingmentDay1;

public class SumOfTheSquare {
    public static void show()
    { 
    	int squr=0;
    	int num=0;
    	double count=0;
    	for(int i=1;i<=100;i++)
    	{
    		num=num+i*i;
    		count++;
    	
    	}
    	System.out.println("The sum of No is "+num);
    	double avg=(double)num/count;
    	System.out.println("The averag of no is "+avg);
    }
}
