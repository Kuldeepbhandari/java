package AssingmentDay1;

public class Pattern {
  public static void show()
  {
	  for(int i=1;i<=6;i++)
	  {
		  for(int j=i;j<=6;j++)
		  {		if(i==1||i==7||j==1||j==2||j==5||j==6)
		  {
				System.out.print("*");
		  }
		  else{
			  System.out.println(" ");
		  }
		 
			
		  }
		  System.out.println(" ");
	  }
	 
  }
}
