import java.util.Scanner;


public class Binary2Octal {

	int sum=0;
	double base=2;
	int temp2=0;
public void show(String bin){
	for(int i=bin.length()-1;i>=0;i--)
	{
		if(bin=="0" && bin=="1")
		{
			sum=Integer.parseUnsignedInt(bin, bin.charAt(i));
			sum++;
		}
	}
	System.out.println(sum);
}

}
